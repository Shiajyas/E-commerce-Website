<script>
document.addEventListener("DOMContentLoaded", () => {
    const searchInput = document.getElementById("headerSearchInput");
    const clearIcon = document.getElementById("clearSearchIcon");

    // Get search query from URL
    const params = new URLSearchParams(window.location.search);
    const searchQuery = params.get("search") || "";

    // Set input value from URL
    searchInput.value = searchQuery;

    // Show clear icon if search exists
    if (searchQuery) {
        clearIcon.style.display = "inline";
    }

    // Show/hide clear icon while typing
    searchInput.addEventListener("input", () => {
        if (searchInput.value.trim()) {
            clearIcon.style.display = "inline";
        } else {
            clearIcon.style.display = "none";
        }
    });
});

// Perform search
function handleHeaderSearch() {
    const searchInput = document.getElementById("headerSearchInput");
    const query = searchInput.value.trim();
    const params = new URLSearchParams(window.location.search);

    if (query) {
        params.set("search", query);
    } else {
        params.delete("search");
    }

    params.delete("page"); // reset pagination
    window.location.href = `/shop?${params.toString()}`;
}

// Clear search explicitly
function clearHeaderSearch() {
    const searchInput = document.getElementById("headerSearchInput");
    searchInput.value = "";

    const params = new URLSearchParams(window.location.search);
    params.delete("search");
    params.delete("page"); // reset pagination

    window.location.href = `/shop?${params.toString()}`;
}
</script>

<script>
document.addEventListener("DOMContentLoaded", () => {
    const searchInput = document.getElementById("headerSearchInput");
    const clearIcon = document.getElementById("clearSearchIcon");

    // Show/hide clear icon
    const toggleClearIcon = () => {
        clearIcon.style.display = searchInput.value.trim() ? "inline" : "none";
    };

    searchInput.addEventListener("input", toggleClearIcon);
    toggleClearIcon(); // run on page load if search exists

    // Highlight selected price button
    const params = new URLSearchParams(window.location.search);
    const gt = params.get("gt");
    const lt = params.get("lt");

    document.querySelectorAll(".price-button").forEach(btn => {
        btn.classList.remove("active");
        const btnGt = btn.getAttribute("data-gt") || "";
        const btnLt = btn.getAttribute("data-lt") || "";
        if ((gt === btnGt && lt === btnLt) || (!gt && !lt && btnGt === "" && btnLt === "")) {
            btn.classList.add("active");
        }
    });
});

// Handle search submission
function handleHeaderSearch() {
    const input = document.getElementById("headerSearchInput");
    const query = input.value.trim();
    const params = new URLSearchParams(window.location.search);

    if (query) params.set("search", query);
    else params.delete("search"); // only delete if input cleared

    params.delete("page"); // reset pagination
    window.location.href = `/shop?${params.toString()}`;
}

// Clear search explicitly
function clearHeaderSearch() {
    const input = document.getElementById("headerSearchInput");
    input.value = "";
    const params = new URLSearchParams(window.location.search);
    params.delete("search"); // remove search param
    params.delete("page");   // reset pagination
    window.location.href = `/shop?${params.toString()}`;
}

// Sort, category, brand filters
function updateQuery(paramsObj) {
    const url = new URL(window.location.href);
    Object.keys(paramsObj).forEach(k => {
        if (!paramsObj[k]) url.searchParams.delete(k);
        else url.searchParams.set(k, paramsObj[k]);
    });
    url.searchParams.delete("page");
    window.location.href = url.toString();
}

function handleSortChange(v) {
    updateQuery({ sort: v === "clear" ? null : v });
}

function handleCategoryChange(v) {
    updateQuery({ category: v });
}

function handleBrandChange(v) {
    updateQuery({ brand: v });
}

// Price filter — **does NOT remove search query**
function handlePriceFilter(gt, lt) {
    const params = new URLSearchParams(window.location.search);

    if (gt === null || lt === null) {
        params.delete("gt");
        params.delete("lt");
    } else {
        params.set("gt", gt);
        params.set("lt", lt);
    }

    params.delete("page"); // reset pagination
    window.location.href = `/shop?${params.toString()}`;
}
</script>




<script>

function getPaginationLink(page) {
        const urlParams = new URLSearchParams(window.location.search);
        const category = urlParams.get('category');
        const brand = urlParams.get('brand');
        const sort = urlParams.get('sort');

        const params = new URLSearchParams();
        if (category) params.set('category', category);
        if (brand) params.set('brand', brand);
        if (sort) params.set('sort', sort);
        params.set('page', page);

        return `/shop?${params.toString()}`;
    }

    function addToCart(prodId) {

        var quantity = 1;
        $.ajax({
            url: `/addToCart?id=${prodId}`,
            method: "post",
            data: {
                quantity: quantity
            },
            success: (res) => {
                if (res.status === true) {

                    Swal.fire({
                        title: 'Added to Cart!',
                        text: 'Continue Shopping',
                        icon: 'success',
                        timer: 2000,
                        showConfirmButton: false
                    }).then(() => { location.reload(); });
                } else if (res.status === "Out of stock") {
                    Swal.fire({
                        title: 'Stock!!',
                        text: "The product is out of stock",
                        icon: 'error', // Corrected from 'fail' to 'error'
                        timer: 2000,
                    })
                } else {
                    Swal.fire({
                        title: 'You need to Login first',
                        text: 'Continue Shopping',
                        icon: 'warning',
                        timer: 2000,
                        showConfirmButton: false
                    }).then(() => { window.location.href = '/login' });
                }
            },
            error: (err) => {
                console.error(err, "hello");
                res.alert("error occured")
            }
        });
    }

</script>



<script>

function getCurrentPageFromURL(element) {
    
    const url = window.location.href;
    const match = url.match(/page=(\d+)/);

    if (match && match[1]) {
        return parseInt(match[1], 10);
    }

    return 1;
}


    
</script>



